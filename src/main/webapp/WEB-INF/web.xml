<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">
    <servlet>
        <servlet-name>Hello</servlet-name>
        <servlet-class>com.naver.svngit.HelloServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>svn</servlet-name>
        <servlet-class>com.naver.svngit.SVNGitServlet</servlet-class>
        <init-param>
            <param-name>SVNParentPath</param-name>
            <param-value>/home/nori/mysrc/svn-to-git-jetty/repo/npcode</param-value>
        </init-param>
    </servlet>

    <servlet>
        <servlet-name>svn2</servlet-name>
        <servlet-class>org.tmatesoft.svn.core.internal.server.dav.DAVServlet</servlet-class>
        <init-param>
            <param-name>SVNParentPath</param-name>
            <param-value>/home/nori/mysrc/svn-to-git-jetty/repo/svn2</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>svn</servlet-name>
        <url-pattern>/svn/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>svn2</servlet-name>
        <url-pattern>/svn2/*</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Hello</servlet-name>
        <url-pattern>/hello/*</url-pattern>
    </servlet-mapping>

</web-app>
